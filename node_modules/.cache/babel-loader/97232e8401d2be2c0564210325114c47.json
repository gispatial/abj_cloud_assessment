A,CAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,IAJA,EAIA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,4BAAA,EAAA,IAAA;AACA;AANA,KAPA;AAeA,IAAA,gBAAA,EAAA;AACA,MAAA,GADA,iBACA;AACA,eAAA,KAAA,YAAA,CAAA,KAAA,CAAA,EAAA,CAAA;AACA,OAHA;AAIA,MAAA,GAJA,eAIA,IAJA,EAIA;AACA,aAAA,MAAA,CAAA,QAAA,CAAA,yBAAA,EAAA,IAAA;AACA;AANA,KAfA;AAuBA,IAAA,SAvBA,uBAuBA;AACA,aAAA;AAAA,sBAAA,KAAA,MAAA,CAAA,KAAA,CAAA,cAAA,KAAA,UAAA,IAAA,KAAA,WAAA,KAAA,KAAA,MAAA,CAAA,KAAA,CAAA,KAAA,KAAA,MAAA,GAAA,SAAA,GAAA,MAAA;AAAA,OAAA;AACA;AAzBA,GArBA;AAgDA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,oBACA,GADA,EACA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,wBAAA,EAAA,KAAA;AACA,WAAA,yBAAA,GAAA,KAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA,CAAA,KAAA,CAAA,GAAA,EAAA,KAAA,CAAA,YAAA,CAAA,CAAA;AACA,KALA;AAMA,IAAA,aANA,yBAMA,IANA,EAMA;AACA,WAAA,MAAA,CAAA,QAAA,CAAA,mBAAA,EAAA;AAAA,QAAA,GAAA,EAAA,IAAA,CAAA,GAAA;AAAA,QAAA,GAAA,EAAA,CAAA,IAAA,CAAA;AAAA,OAAA,EADA,CAEA;AACA,KATA;AAUA,IAAA,OAAA,EAAA,mBAAA;AACA,WAAA,yBAAA,GAAA,KAAA;AACA,KAZA;AAaA,IAAA,uBAbA,mCAaA,KAbA,EAaA;AACA;AACA,WAAA,MAAA,CAAA,MAAA,CAAA,wBAAA,EAAA,KAAA,GAAA,IAAA,GAAA,KAAA;AACA;AAhBA,GAhDA;AAkEA,EAAA,UAAA,EAAA;AACA,qBAAA;AACA,MAAA,IAAA,EAAA,cAAA,EAAA,EAAA,OAAA,EAAA;AACA,YAAA,MAAA,GAAA,OAAA,CAAA,SAAA,CAAA,MAAA;;AACA,YAAA,OAAA,GAAA,SAAA,OAAA,CAAA,CAAA,EAAA;AACA,cAAA,MAAA,IAAA,CAAA,EAAA,CAAA,QAAA,CAAA,CAAA,CAAA,MAAA,CAAA,IAAA,EAAA,KAAA,CAAA,CAAA,MAAA,EAAA;AACA,YAAA,OAAA,CAAA,KAAA,CAAA,CAAA;AACA;AACA,SAJA;;AAKA,QAAA,EAAA,CAAA,mBAAA,GAAA,OAAA;AACA,QAAA,QAAA,CAAA,gBAAA,CAAA,OAAA,EAAA,OAAA;AACA,OAVA;AAYA,MAAA,MAAA,EAAA,gBAAA,EAAA,EAAA;AACA,QAAA,QAAA,CAAA,mBAAA,CAAA,OAAA,EAAA,EAAA,CAAA,mBAAA;AACA,QAAA,EAAA,CAAA,mBAAA,GAAA,IAAA;AAEA;AAhBA;AADA;AAlEA,CAAA","sourcesContent":["<template>\n  <div class=\"navbar-bookmarks flex items-center\">\n    <!-- STARRED PAGES - FIRST 10 -->\n    <ul class=\"vx-navbar__starred-pages\">\n      <draggable v-model=\"starredPagesLimited\" :group=\"{name: 'pinList'}\" class=\"flex cursor-move\">\n        <li class=\"starred-page\" v-for=\"page in starredPagesLimited\" :key=\"page.url\">\n          <vx-tooltip :text=\"page.title\" position=\"bottom\" delay=\".3s\">\n            <feather-icon :svgClasses=\"['h-6 w-6 stroke-current', textColor]\" class=\"p-2 cursor-pointer\" :icon=\"page.icon\" @click=\"$router.push(page.url).catch(() => {})\" />\n          </vx-tooltip>\n        </li>\n      </draggable>\n    </ul>\n\n    <!-- STARRED PAGES MORE -->\n    <div class=\"vx-navbar__starred-pages--more-dropdown\" v-if=\"starredPagesMore.length\">\n      <vs-dropdown vs-custom-content vs-trigger-click class=\"cursor-pointer\">\n        <feather-icon icon=\"ChevronDownIcon\" svgClasses=\"h-4 w-4\" class=\"cursor-pointer p-2\"></feather-icon>\n        <vs-dropdown-menu>\n          <ul class=\"vx-navbar__starred-pages-more--list\">\n            <draggable v-model=\"starredPagesMore\" :group=\"{name: 'pinList'}\" class=\"cursor-move\">\n              <li class=\"starred-page--more flex items-center cursor-pointer\" v-for=\"page in starredPagesMore\" :key=\"page.url\" @click=\"$router.push(page.url).catch(() => {})\">\n                <feather-icon class=\"ml-2 mr-1\" :icon=\"page.icon\" :svgClasses=\"['h-5 w-5 stroke-current', textColor]\"></feather-icon>\n                <span class=\"px-2 pt-2 pb-1\">{{ page.title }}</span>\n              </li>\n            </draggable>\n          </ul>\n        </vs-dropdown-menu>\n      </vs-dropdown>\n    </div>\n\n    <div class=\"bookmark-container\">\n      <feather-icon icon=\"\" :svgClasses=\"['stoke-current text-warning', textColor]\" class=\"cursor-pointer p-2\" @click.stop=\"showBookmarkPagesDropdown = !showBookmarkPagesDropdown\" />\n      <div v-click-outside=\"outside\" class=\"absolute bookmark-list w-1/3 xl:w-1/4 mt-4\" v-if=\"showBookmarkPagesDropdown\">\n        <vx-auto-suggest\n          ref=\"bookmarkAutoSuggest\"\n          :autoFocus=\"true\"\n          :data=\"navbarSearchAndPinList\"\n          :initalData=\"{pages: starredPagesLimited.concat(starredPagesMore)}\"\n          :searchLimit=\"5\"\n          placeholder=\"Explore Tripcarte.Asia......\"\n          inputClassses=\"w-full\"\n          show-action\n          show-pinned\n          hideGroupTitle\n          background-overlay\n          @input=\"hnd_search_query_update\"\n          @selected=\"selected\">\n\n          <!-- Pages Suggestion -->\n          <template v-slot:pages=\"{ suggestion }\">\n            <div class=\"flex items-center justify-between\">\n              <div class=\"flex items-end leading-none py-1\">\n                <feather-icon :icon=\"suggestion.icon\" svgClasses=\"h-5 w-5\" class=\"mr-4\" />\n                <span class=\"mt-1\">{{ suggestion.title }}</span>\n              </div>\n              <feather-icon\n                icon=\"StarIcon\"\n                :svgClasses=\"[{'text-warning': suggestion.is_bookmarked}, 'h-5 w-5 stroke-current mt-1']\"\n                @click.stop=\"actionClicked(suggestion)\" />\n            </div>\n          </template>\n\n        </vx-auto-suggest>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport draggable     from 'vuedraggable'\nimport VxAutoSuggest from '@/components/vx-auto-suggest/VxAutoSuggest.vue'\n\nexport default {\n  props: {\n    navbarColor: {\n      type: String,\n      default: \"#fff\",\n    },\n  },\n  components: {\n    draggable,\n    VxAutoSuggest\n  },\n  data() {\n    return {\n      showBookmarkPagesDropdown : false,\n    }\n  },\n  watch: {\n    '$route'() {\n      if (this.showBookmarkPagesDropdown) this.showBookmarkPagesDropdown = false\n    }\n  },\n  computed: {\n    navbarSearchAndPinList() {\n      return {pages: this.$store.state.navbarSearchAndPinList[\"pages\"]}\n    },\n    starredPages() {\n      return this.$store.state.starredPages\n    },\n    starredPagesLimited: {\n      get() {\n        return this.starredPages.slice(0, 10)\n      },\n      set(list) {\n        this.$store.dispatch('arrangeStarredPagesLimited', list)\n      }\n    },\n    starredPagesMore: {\n      get() {\n        return this.starredPages.slice(10)\n      },\n      set(list) {\n        this.$store.dispatch('arrangeStarredPagesMore', list)\n      }\n    },\n    textColor() {\n      return {'text-white': this.$store.state.mainLayoutType === 'vertical' && this.navbarColor != (this.$store.state.theme === 'dark' ? \"#10163a\" : \"#fff\") }\n    }\n  },\n  methods: {\n    selected(obj) {\n      this.$store.commit('TOGGLE_CONTENT_OVERLAY', false)\n      this.showBookmarkPagesDropdown = false\n      this.$router.push(obj.pages.url).catch(() => {})\n    },\n    actionClicked(item) {\n      this.$store.dispatch('updateStarredPage', { url: item.url, val: !item.is_bookmarked })\n      // this.$refs.bookmarkAutoSuggest.filterData()\n    },\n    outside: function() {\n      this.showBookmarkPagesDropdown = false\n    },\n    hnd_search_query_update(query) {\n      // Show overlay if any character is entered\n      this.$store.commit('TOGGLE_CONTENT_OVERLAY', query ? true : false)\n    }\n  },\n  directives: {\n    'click-outside': {\n      bind: function(el, binding) {\n        const bubble = binding.modifiers.bubble\n        const handler = (e) => {\n          if (bubble || (!el.contains(e.target) && el !== e.target)) {\n            binding.value(e)\n          }\n        }\n        el.__vueClickOutside__ = handler\n        document.addEventListener('click', handler)\n      },\n\n      unbind: function(el) {\n        document.removeEventListener('click', el.__vueClickOutside__)\n        el.__vueClickOutside__ = null\n\n      }\n    }\n  }\n}\n\n</script>\n"],"sourceRoot":"src/layouts/components/navbar/components"}]}